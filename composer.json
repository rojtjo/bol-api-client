{
    "name": "rojtjo/bol-api-client",
    "type": "library",
    "description": "Bol.com Retailer API client v7",
    "keywords": [
        "bol",
        "api",
        "client",
        "bol.com"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "illuminate/collections": "^9.26",
        "league/csv": "^9.0",
        "psr/http-client": "^1.0",
        "psr/simple-cache": "^3.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Rojtjo\\Bol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
